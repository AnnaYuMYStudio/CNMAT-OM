
(in-package :cnmat)

(defun test-canon (mylist)
  (let ((canon (list (first mylist))))
    (loop for elem in (cdr mylist) do
          (if (canon-query (cons elem canon))
              (push elem canon)))
    (reverse canon)))

(defun is-rotation? (l1 l2)
  (let ((ok nil))
    (loop for i from 0 to (1- (length l1))
          while (not ok)
          do (setf ok (equal (rotate l1 i) l2)))
    ok))

         